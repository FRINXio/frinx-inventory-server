{
  "name": "@frinx/inventory-server",
  "version": "0.0.5",
  "engines": {
    "node": "^14.15.0"
  },
  "author": "Frinx.io",
  "license": "MIT",
  "scripts": {
    "build:copy": "cp -r prisma ./build/pack",
    "build:babel": "cross-env NODE_ENV=production babel --extensions .ts ./src --out-dir ./build/babel",
    "build:pack": "cross-env NODE_ENV=production ncc build build/babel/index.js -o build/pack",
    "build:clear": "rimraf build",
    "build": "run-s build:clear build:babel build:pack build:copy",
    "dev": "cross-env DEBUG='frinx-inventory-server' DEBUG_COLORS=true DOTENV_CONFIG_PATH=./.env nodemon --exec 'node -r dotenv/config -r ./babel-register-ts.js' -- ./src/index.ts",
    "dev:migrate": "prisma migrate save --experimental -c && prisma migrate up --experimental -c",
    "prisma:generate": "prisma generate",
    "lint": "eslint --ext .ts 'src/**/*.ts' --max-warnings 0",
    "type-check": "tsc --noEmit",
    "formatter": "prettier --write '*.md' '**/*.ts' '**/*.js' '**/*.json' ",
    "formatter:check": "prettier -l '*.md' '**/*.ts' '**/*.js' '**/*.json' "
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.13.16",
    "@frinx/eslint-config-typescript-base": "^14.2.1-build.4",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@vercel/ncc": "^0.29.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prisma": "^2.27.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@prisma/client": "^2.27.0",
    "@types/express": "^4.17.12",
    "@types/lodash": "^4.14.172",
    "@types/node-fetch": "^2.5.11",
    "@types/pino": "^6.3.8",
    "@types/url-join": "^4.0.1",
    "apollo-server-core": "^3.1.2",
    "apollo-server-express": "^2",
    "base64url": "^3.0.1",
    "express": "^4.17.1",
    "fp-ts": "^2.10.5",
    "graphql": "^15.5.1",
    "graphql-relay": "^0.8.0",
    "helmet": "^4.6.0",
    "io-ts": "^2.2.16",
    "lodash": "^4.17.21",
    "nexus": "^1.1.0",
    "node-fetch": "^2.6.1",
    "pino": "^6.11.3",
    "pino-pretty": "^5.0.2",
    "url-join": "^4.0.1"
  }
}
